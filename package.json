{
  "name": "prismic-cli",
  "description": "Command line to bootstrap prismic projects.",
  "version": "3.6.2",
  "author": "prismic.io developers",
  "bin": {
    "prismic": "bin/prismic.js",
    "prismic-cli": "./bin/run"
  },
  "bugs": "https://github.com/prismicio/prismic-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "adm-zip": "^0.4.13",
    "axios": "^0.19.0",
    "babel-runtime": "~6.26.0",
    "command-line-commands": "~3.0.1",
    "command-line-usage": "~6.1.0",
    "debug": "^4.1.1",
    "deep-equal": "^1.1.0",
    "gittar": "^0.1.1",
    "inquirer": "~7.0.0",
    "lodash": "^4.17.15",
    "open": "^7.0.0",
    "prismic.io": "~3.6.1",
    "request": "~2.88.0",
    "shelljs": "~0.8.3",
    "tmp": "~0.1.0",
    "tslib": "^1",
    "vue-docgen-api": "^3.26.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/debug": "^4.1.5",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12",
    "@types/request": "^2.48.3",
    "@types/shelljs": "^0.8.6",
    "dotenv": "^8.2.0",
    "globby": "^10",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.7"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/prismicio/prismic-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "prismic-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "preferGlobal": true,
  "repository": "https://github.com/prismicio/prismic-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -p . -t stylish --fix"
  },
  "types": "lib/index.d.ts"
}
