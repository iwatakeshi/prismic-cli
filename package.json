{
  "name": "prismic-cli",
  "description": "Command line to bootstrap prismic projects.",
  "version": "3.6.2",
  "author": "prismic.io developers",
  "bin": {
    "prismic": "./bin/run"
  },
  "bugs": "https://github.com/prismicio/prismic-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.4",
    "@oclif/plugin-help": "^2",
    "babel-runtime": "~6.26.0",
    "debug": "^4.1.1",
    "gitly": "^1.0.4",
    "got": "^10.0.0-alpha.3.2",
    "inquirer": "~7.0.0",
    "open": "^7.0.0",
    "prismic.io": "~3.6.1",
    "shelljs": "~0.8.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/debug": "^4.1.5",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12",
    "@types/shelljs": "^0.8.6",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.7"
  },
  "engines": {
    "node": ">= 10.0",
    "npm": ">= 3.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/prismicio/prismic-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "prismic-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "preferGlobal": true,
  "repository": "https://github.com/prismicio/prismic-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -p . -t stylish",
    "lint:fix": "tslint -p . -t stylish --fix"
  },
  "types": "lib/index.d.ts"
}
